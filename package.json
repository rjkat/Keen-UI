{
  "name": "keen-ui",
  "version": "1.2.1",
  "description": "Keen UI is a Vue.js UI library with a simple API, inspired by Google's Material Design.",
  "main": "dist/keen-ui.js",
  "scripts": {
    "clean": "rimraf ./dist ./lib ./node_modules/.cache",
    "build": "vue-cli-service build --target lib --formats umd,umd-min --name KeenUI --filename keen-ui src/index.js",
    "build:lib": "rimraf ./lib && node build-lib.js",
    "lint": "vue-cli-service lint --no-fix ./src ./stories ./.storybook ./*.js",
    "format": "vue-cli-service lint ./src ./stories ./.storybook ./*.js",
    "test": "npm run lint",
    "postinstall": "patch-package",
    "storybook": "start-storybook -p 6006 --quiet --ci",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JosephusPaye/Keen-UI.git"
  },
  "keywords": [
    "vue",
    "vuejs",
    "material design",
    "components",
    "ui"
  ],
  "files": [
    "dist",
    "lib",
    "src",
    "CHANGELOG.md",
    "LICENCE",
    "README.md",
    "yarn.lock"
  ],
  "author": {
    "name": "Josephus Paye II"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JosephusPaye/Keen-UI/issues"
  },
  "homepage": "https://github.com/JosephusPaye/Keen-UI/",
  "dependencies": {
    "autosize": "^5.0.1",
    "deepmerge": "^4.2.2",
    "fuzzysearch": "^1.0.3",
    "lodash.debounce": "^4.0.8",
    "tippy.js": "^6.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@storybook/addon-actions": "^6.3.8",
    "@storybook/addon-links": "^6.3.8",
    "@storybook/addons": "^6.3.8",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/vue": "^6.1.21",
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-service": "^3.12.1",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.2.2",
    "babel-preset-vue": "^2.0.2",
    "clipboard": "^2.0.8",
    "dedent-js": "^1.0.1",
    "execa": "^5.1.1",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.4.0",
    "prismjs": "^1.24.1",
    "rimraf": "^3.0.2",
    "sass": "^1.39.0",
    "sass-loader": "^12.1.0",
    "vue": "^2.6.14",
    "vue-prism-component": "^2.0.0",
    "vue-template-compiler": "^2.6.14"
  }
}
